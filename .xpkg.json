{
    "links": {
        "imwheelrc": "~/.imwheelrc",
        "interactive.sh": "~/.local/share/bash-hooks/interactive/mouse"
    },
    "install": [
        ["link", "imwheelrc", "~/.imwheelrc"],
        ["link", "interactive.sh", "~/.local/share/bash-hooks/interactive/mouse"]
    ],
    "remove": [
        ["unlink", "imwheelrc", "~/.imwheelrc"],
        ["unlink", "interactive.sh", "~/.local/share/bash-hooks/interactive/mouse"]
    ],
    "dependencies": [
        "elegantchaos/bash-hooks"
    ]
}
